									$(".star-rating").starRating({
										initialRating: 4.0,
										strokeColor: '#ff7c00',
										strokeWidth: 10,
										starSize: 15,
										readOnly: true
									});
								</script>
								<span>{{this.createdDate}}</span>
							</div>
							<p>{{this.content}}</p>
