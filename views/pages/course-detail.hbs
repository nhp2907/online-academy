				{{#each course.reviews}}
					<div class="review">
						<img src="../../{{this.user.image}}"
						     alt="{{this.user.firstName}}">
						<div class="main">
							<div class="name">
								<h2>{{this.user.firstName}} {{this.user.lastName}}</h2>
								<div class="like-dislike">
									<i class="fas fa-thumbs-up"></i>
									<i class="fas fa-thumbs-down"></i>
								</div>
							</div>
